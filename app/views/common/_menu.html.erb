<ul>
  <li><%= link_to "Books", books_path%> </li>
  <li><%= link_to "Genres", genres_path%> </li>
  <% info_pages.each do |page| %>
    <li><%= link_to page.title, permalink_path(page.permalink) %> </li>
  <% end %>
  <% if current_user %>
    <li><%= link_to 'Settings', edit_user_registration_path %> </li>
    <li><%= link_to 'Log out', destroy_user_session_path, method: 'delete' %> </li>
  <% else %>
    <li><%= link_to 'Login', new_user_session_path %> </li>
    <li><%= link_to 'Register', new_user_registration_path %> </li>
  <% end %>
</ul>
<%= form_with(url:search_books_path, method: :get, local: true) do %>
    <%= text_field_tag(:keywords, nil, class: "input", placeholder: "Search for Books") %>
    <%= select_tag :type, options_for_select(@searchGenreOptions) %>
    <%= submit_tag("Search", class: "button is-primary")%>
<% end %>
<nav>
  <ul>
    <li><%= link_to "Cart Total: #{number_to_currency cart[0]}", cart_index_path %>
      <% if cart.present? %>
      <sup><%= cart[1].size %></sup>
      <ul>
        <% cart[1].each do |bookInCart| %>
          <li> <%= bookInCart[1]%> x  <%= link_to bookInCart[0].name,  bookInCart[0] %></li>
        <% end %>

      </ul>
      <% end %>
    </li>
  </ul>
</nav>